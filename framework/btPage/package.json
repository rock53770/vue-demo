{
  "name": "openPage",
  "version": "1.1.0",
  "description": "全平台通用页面间跳转和回传封装组件，支持app内跳转和wap跳转，支持hash模式和history模式跳转",
  "license": "MIT",
  "scripts": {
    "start": "npm run sync",
    "build": "webpack --watch",
    "sync": "browser-sync start --server \"./\" --baseDir --directory true --files \"**/*\" --open \"external\""
  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-core": "~6.22.1",
    "babel-loader": "~7.1.1",
    "babel-plugin-transform-runtime": "~6.22.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~1.3.2",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-latest": "~6.24.1",
    "babel-preset-stage-0": "~6.1.2",
    "babel-register": "~6.22.0",
    "babel-runtime": "~5.8.0",
    "chalk": "^2.3.2",
    "css-loader": "~0.7.0",
    "expose-loader": "~0.7.5",
    "extract-text-webpack-plugin": "~3.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "jquery": "~3.3.1",
    "less": "^3.0.1",
    "less-loader": "^3.0.0",
    "path-to-regexp": "^2.2.0",
    "postcss-loader": "^2.1.4",
    "rimraf": "~2.6.1",
    "script-loader": "~0.7.0",
    "style-loader": "^0.20.3",
    "url-loader": "~0.5.9",
    "vue": "~2.5.16",
    "vue-loader": "~14.2.2",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "~2.2.1"
  }
}
